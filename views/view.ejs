<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="style.css">
  <title>View</title>
</head>
<body>
  <div class="container">
    <div class="layout">
      <div class="photo-slider">
        <div class="banner">
          <img src="imgs/schools/nfhss.jpg">
          <div class="greeting"></div>
        </div>

        <div class="slider-container">
          <div class="slider" data-posts="<%= posts %>">
          </div>
        </div>
        <div class="caption-editor">
          <div class="editor caption"></div>
        </div>
      </div>

      <div class="side-pannel">
        <div class="date-container">
          <div class="date-bubble">
            <p class="date"><span></span><span></span></p>
            <p class="time"><span></span><span></span></p>
          </div>
        </div>

        <div class="events">
          <% for (event of events) { %>
            
          <% } %>
        </div>
      </div>
      <div class="scores-container">
        <div class="scores" data-scores="<%= scores %>"></div>
      </div>
    </div>
  </div>
  <script src="//cdn.quilljs.com/1.3.6/quill.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script src="js/main.js"></script>
  <script src="js/home/slider.js"></script>
  <script src="js/home/time.js"></script>
  <script src="js/home/scores.js"></script>
  <script>
    getPost()
    document.querySelector(".ql-editor").contentEditable = false
  </script>
</body>
</html>